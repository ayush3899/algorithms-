//Ugly numbers are numbers whose only prime factors are 2, 3 or 5. The sequence 1, 2, 3, 4, 5, 6, 8, 9, 10, 12, 15, â€¦

# include<bits/stdc++.h>
using namespace std;

int ugly(int n)
{
int u[n];
int i2=0;
int i3=0;
int i5=0;

int n2=2;
int n3=3;
int n5=5;

int nn = 1;

u[0]=1;

for(int i=1;i<n;i++)
{
	nn=min(n2,min(n3,n5));
	u[i]=nn;
	
	if(nn==n2)
	{
		i2++;
		n2=u[i2]*2;
	}
		
	if(nn==n3)
	{
		i3++;
		n3=u[i3]*3;
	}
		
	if(nn==n5)
	{
		i5++;
		n5=u[i5]*5;
	}
		
}
return nn;
}
	
int  main()
{
	int n;
	cin>>n;
	cout<<ugly(n)<<"\n";

	return 0;
	
}
