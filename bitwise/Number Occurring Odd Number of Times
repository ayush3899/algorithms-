
#include <bits/stdc++.h>
using namespace std;

int get(int ar[], int n)
{
	int res = 0; 
	for (int i = 0; i <n; i++)	 
		res = res ^ ar[i];
	
	return res;
}

int main()
{
    int n;
    cin>>n;
    iny a[n+2];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
	cout << get(a, n);
	
	return 0;
}
